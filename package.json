{
  "name": "blueprint",
  "version": "0.0.0",
  "scripts": {
    "init": "npm i && cd functions && npm i && cd ..",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build:ssr": "ng run blueprint:server && npm run webpack:ssr",
    "build:ssr:prod": "ng run blueprint:server:production && npm run webpack:ssr",
    "webpack:ssr": "webpack --config webpack.server.config.js",
    "serve:ssr": "node dist/blueprint-webpack/server.js",
    "build": "ng build && npm run copy:hosting && npm run build:ssr && npm run build:functions",
    "build:prod": "ng build --prod && npm run copy:hosting && npm run build:ssr:prod && npm run build:functions",
    "copy:hosting": "cp -r ./dist/blueprint/* ./public && rm ./public/index.html",
    "build:functions": "npm run --prefix functions build",
    "start:fire": "firebase serve --project test-firebase-9eeed"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/common": "^7.2.11",
    "@angular/compiler": "^7.2.11",
    "@angular/core": "^7.2.11",
    "@angular/fire": "^5.1.2",
    "@angular/forms": "^7.2.11",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "^7.2.11",
    "@angular/platform-browser-dynamic": "^7.2.11",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "^7.2.11",
    "core-js": "^2.5.4",
    "firebase": "^5.9.2",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.7",
    "@angular/compiler-cli": "^7.2.11",
    "@angular/language-service": "^7.2.11",
    "@netbasal/spectator": "^3.6.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "express": "^4.16.4",
    "fast-check": "^1.13.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "^5.3.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.3.0",
    "ws": "^6.2.1",
    "xhr2": "^0.1.4"
  }
